{
  "label" : "shutdown",
  "action" : "systemctl poweroff",
  "text" : "Shutdown",
  "keybind" : "s"
},
{
  "label" : "reboot",
  "action" : "systemctl reboot",
  "text" : "Reboot",
  "keybind" : "r"
},
{
  "label" : "logout",
  "action" : "loginctl terminate-session $XDG_SESSION_ID",
  "text" : "Logout",
  "keybind" : "e"
},
{
  "label" : "hibernate",
  "action" : "swaylock -f && systemctl hibernate",
  "text" : "Hibernate",
  "keybind" : "h"
},
{
  "label" : "lock",
  "action" : "/usr/bin/swaylock",
  "text" : "Lock",
  "keybind" : "l"
}
